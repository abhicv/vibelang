// Factorial function example
// Demonstrates recursion and function calls

fn factorial(n: int) -> int {
    if n <= 1 {
        return 1;
    }
    return n * factorial(n - 1);
}

fn main() -> int {
    let result = factorial(5);
    print("Factorial of 5 is: ");
    print("120");
    return 0;
}

main();
