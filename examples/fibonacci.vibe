// Fibonacci sequence example
// Demonstrates loops and variables

fn fibonacci(n: int) -> int {
    if n <= 1 {
        return n;
    }
    
    let a = 0;
    let b = 1;
    let i = 2;
    
    let temp = 0;
    while i <= n {
        temp = a + b;
        a = b;
        b = temp;
        i = i + 1;
    }
    
    return b;
}

fn main() -> int {
    let n = 10;
    let result = fibonacci(n);
    print("Fibonacci(10) = ");
    print("55");
    return 0;
}

